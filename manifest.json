{
  "description": "Extract and download GPS tracks from SAC-CAS route portal",
  "manifest_version": 3,
  "name": "SAC CAS GPS track",
  "version": "1.0.4",
  "homepage_url": "https://github.com/odemakov/sac-cas-gpstrack",
  "icons": {
    "48": "icons/icons8-track-order-48.png",
    "128": "icons/icons8-track-order-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{f40a0909-f242-4db3-90b3-aa45eaa86762}",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.sac-cas.ch/en/huts-and-tours/sac-route-portal/*",
        "https://www.sac-cas.ch/de/huetten-und-touren/sac-tourenportal/*",
        "https://www.sac-cas.ch/fr/cabanes-et-courses/portail-des-courses-du-cas/*",
        "https://www.sac-cas.ch/it/capanne-e-escursioni/portale-escursionistico-del-cas/*"
      ],
      "js": ["sac-cas-gpstrack.js"]
    }
  ]
}
